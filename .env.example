SOURCE_CONNECTION=oracle+oracledb://user:pass@host:1521/service
TARGET_CONNECTION=postgresql+psycopg://accelerator:accelerator@localhost:5432/accelerator

# LLM advisor (optional)
ENABLE_LLM_ADVISOR=false
LLM_MODEL=gemini-1.5-pro
GOOGLE_API_KEY=

# Source discovery / sampling
INCLUDE_SAMPLE_ROWS=true
SAMPLE_ROW_LIMIT=3

# Embeddings (Hugging Face sentence-transformers)
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
HF_TOKEN=
HF_TOKEN_ENV_VAR=HF_TOKEN
VECTOR_DIM=384

# Target vector storage
VECTOR_TABLE=rag_documents

# Generated migration execution
RUN_CONTAINERIZED_MIGRATION=false
CONTAINER_RUNTIME=podman
